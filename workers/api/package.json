{
  "name": "vibecov-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "db:create": "wrangler d1 create vibecov-db",
    "db:migrate": "wrangler d1 migrations apply vibecov-db --local",
    "db:migrate:production": "wrangler d1 migrations apply vibecov-db --remote",
    "cli": "tsx src/cli.ts",
    "example:client": "tsx examples/client-usage.ts",
    "example:fixtures": "tsx examples/test-fixtures.ts",
    "quickstart": "./scripts/quickstart.sh"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "wrangler": "^3.94.0"
  },
  "dependencies": {
    "hono": "^4.6.14"
  }
}

{
  "name": "vibecov",
  "version": "0.1.0",
  "description": "SaaS platform for tracking and maintaining coding aesthetics across repositories",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && pnpm dev",
    "dev:backend": "cd backend && pnpm dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && pnpm build",
    "build:backend": "cd backend && pnpm build"
  },
  "keywords": [
    "code-quality",
    "code-aesthetics",
    "style-guide",
    "vibe-coding",
    "saas",
    "github-integration",
    "ai-code-review"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjwalters/vibecov"
  },
  "homepage": "https://vibecov.com",
  "packageManager": "pnpm@10.18.2",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
